require "bandit";

sub bandit0 {
    my($user, $host, $port, $pass)=@_;
    my $ssh=bandit_($user, $host, $port, $pass);
    my($stdout, $stderr, $exit) = $ssh->cmd("ls");
	my($stdout, $stderr, $exit) = $ssh->cmd("cat ".$stdout);

	print $stdout;
	
}

bandit0("bandit0", "bandit.labs.overthewire.org", 2220, "bandit0");



